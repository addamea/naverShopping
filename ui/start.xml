<?xml version="1.0" encoding="utf-8"?>
<MOML version="1.1.8" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.applusform.com/xsd/moml_ui_1.1.8.xsd">

	<UILAYOUT portrait="320,480" landscape="320,480">
		<WINDOW layout="0,0,320,480" img="white" align="linear:vertical">
			<WINDOW layout="320,30" align="linear:subCenter">
				<EDIT id="search" layout="235,30" placeHolder="검색어를 입력하세요."/>
				<BUTTON layout="60,30" text="검색" onClick="function.search" />
			</WINDOW>
			<AGATEWEBVIEW id="searchResult" control="false"/>
		</WINDOW>
	</UILAYOUT>

	<FUNCTION id="search">
		<CMD cmd="url = 'http://m.shopping.naver.com/search/all_search.nhn?query='+string.encode(search.text, 'percent')+'&amp;frm=NVSHSRC&amp;cat_id=&amp;sb=true'"/>
		<CMD cmd="searchResult.src = url"/>
		<CMD cmd="device.log(url)"/> 
	</FUNCTION>
</MOML>


